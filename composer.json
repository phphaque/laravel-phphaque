{
  "name": "phphaque/laravel-phphaque",
  "description": "A Laravel package to manage phphaque",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Phphaque\\LaravelPhphaque\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phphaque\\LaravelPhphaque\\PhphaqueServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "phphaque",
      "email": "innosoftx@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
}